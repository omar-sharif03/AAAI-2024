"source":["def text_length_distribution(dataset, verbose):\n","    dataset['cleaned'] = dataset['Text'].apply(cleaning_data, bool_stopwords = verbose)\n","    # Let's look at the word length for each text in the data and record these\n","    ll = []\n","    for i in range(0, len(dataset['cleaned'])):\n","          x = len(dataset['cleaned'][i].split())\n","          ll.append(x)\n","    dataset['char_length'] = ll\n","    print(dataset['cleaned'][0], dataset['char_length'][0])\n","    # look at the distribution plot for text length\n","    import matplotlib\n","    #matplotlib.rc_file_defaults()\n","    frequency = dict()\n","    for i in ll:\n","        frequency[i] = frequency.get(i, 0)+1\n","\n","    plt.figure(figsize = (6,4))\n","    plt.bar(frequency.keys(), frequency.values(), color =\"b\")\n","    plt.xlim(0, 500)\n","    plt.xlabel('Length of the posts')\n","    plt.ylabel('Frequency')\n","\n","    if verbose == True:\n","      plt.title(f'Text Length-Frequency Distribution (without stowords))')\n","      plt.savefig('text-frequency(without stopwords).png',dpi = 400,bbox_inches ='tight')\n","    else:\n","      plt.title(f'Text Length-Frequency Distribution (with stowords))')\n","      plt.savefig('text-frequency(with stopwords).png',dpi = 400,bbox_inches ='tight')\n","      #plt.savefig(path+'len_dist.png',dpi = 1000,bbox_inches ='tight')\n","    plt.show()\n","\n","    print(f\"Maximum Length of a Post: {max(ll)}\")\n","    print(f\"Minimum Length of a Post: {min(ll)}\")\n","    print(f\"Average Length of a Post: {round(np.mean(ll),2)}\")\n","    return dataset"],"metadata":{"id":"ro6OemSGUKV7","executionInfo":{"status":"ok","timestamp":1689570262288,"user_tz":240,"elapsed":1,"user":{"displayName":"Omar Sharif","userId":"07937204899122819530"}}},"execution_count":60,"outputs":[]},{"cell_type":"code","source":["# Find length- frequency distribution for the dataset\n","verbose =  [True, False]\n","for i in verbose:\n","    data=text_length_distribution(data