async function convertToInteractive(key) {\n","          const element = document.querySelector('#df-ab55f5f1-0a54-4c36-8ced-060aafaa9dc9');\n","          const dataTable =\n","            await google.colab.kernel.invokeFunction('convertToInteractive',\n","                                                     [key], {});\n","          if (!dataTable) return;\n","\n","          const docLinkHtml = 'Like what you see? Visit the ' +\n","            '<a target=\"_blank\" href=https://colab.research.google.com/notebooks/data_table.ipynb>data table notebook</a>'\n","            + ' to learn more about interactive tables.';\n","          element.innerHTML = '';\n","          dataTable['output_type'] = 'display_data';\n","          await google.colab.output.renderOutput(dataTable, element);\n","          const docLink = document.createElement('div');\n","          docLink.innerHTML = docLinkHtml;\n","          element.appendChild(docLink);\n","        }\n","      </script>\n","    </div>\n","  </div>\n"]},"metadata":{},"execution_count":62}]},{"cell_type":"markdown","source":["## Word Distribution per Class"],"metadata":{"id":"NTCiQf3xmKUY"}},{"cell_type":"code","source":["from sklearn.feature_extraction.text import TfidfVectorizer\n","import pandas as pd\n","\n","def tf_idf_top_words(label, data):\n","    documents = list(data[data[label]==1].cleaned)\n","    print(len(documents))\n","    # Initialize the TfidfVectorizer\n","    vectorizer = TfidfVectorizer()\n","\n","    # Fit and transform the documents to get the TF-IDF matrix\n","    tfidf_matrix = vectorizer.fit_transform(documents)\n","\n","    # Get the feature names (words) in the documents\n","    words = vectorizer.get_feature_names_out()\n","\n","    # Calculate the sum of TF-IDF scores for each word\n","    word_tfidf_scores = tfidf_matrix.sum(axis=0)\n","\n","    # Convert the TF-IDF scores to a list\n","    word_tfidf_scores_list = word_tfidf_scores.tolist()[0]\n","\n","    # Create a dictionary to store word: TF-IDF score pairs\n","    word_tfidf_dict = dict(zip(words, word_tfidf_scores_list))\n","\n","    # Sort the dictionary by the TF-IDF scores in descending order\n","    sorted_word_tfidf = sorted(word_tfidf_dict.items(), key=lambda x: x[1], reverse=True)\n","\n","    # Get the top 10 frequent words based on TF-IDF score\n","    top_10_words = sorted_word_tfidf[:70]\n","\n","    # Print the top 10 frequent words and their corresponding TF-IDF scores\n","    for word, tfidf_score in top_10_words:\n","        print(f\"{word}, {tfidf_score:.4f}\")"],"metadata":{"id":"bpch7vTa0ieX","executionInfo":{"status":"ok","timestamp":1689570281659,"user_tz":240,"elapsed":290,"user":{"displayName":"Omar Sharif","userId":"07937204899122819530"}}},"execution_count":63,"outputs":[]},{"cell_type":"code","source":["def data_statistics(data, class_names):\n","  words_per_class = []\n","  unique_words_per_class = []\n","  documents_per_class = []\n","  max_text_length = []\n","  avg_words = []\n","  frqnt_words = {}\n","\n","  for label in class_names:\n","    ## Word list from the clean dataset\n","    word_list = [word.strip().lower() for t in list(data[data[label]==1].cleaned) for word in t.strip().split()]\n","    tf_idf_top_words(label, data)\n","    counts = dict()\n","    for w in word_list:\n","      counts[w] = counts.get(w, 0)+1\n","\n","    mx = 0\n","    for i in range (0, len(data)):\n","        if data[label][i]==1:\n","          cl = data['char_length'][i]\n","          if cl>mx:\n","            mx = cl\n","\n","    max_text_length.append(mx)\n","    # Sort the word list on descending order in terms of frequency\n","    ordered = sorted(counts.items(), key= lambda item: item[1],reverse = True)\n","     # Total words per class\n","    words_per_class.append(len(word_list))\n","    # Unique words per class\n","    unique_words_per_class.append(len(np.unique(word_list)))\n","    # Number of documents per class\n","    documents_per_class.append(len(list(data[data[label]==1].cleaned)))\n","    avg = len(word_list)/len(list(data[data[label]==1].cleaned))\n","    avg_words.append(avg)\n","\n","    print(\"\\n\\nClass name: {}\".format(label))\n","    print(\"Number of Documents: {}\".format(len(list(data[data[label]==1].cleaned))))\n","    print(\"Number of words: {}\".format(len(word_list)))\n","    print(\"Number of unique words: {}\".format(len(np.unique(word_list))))\n","    print(\"Max text length: {}\".format(mx))\n","    print(\"Average Words in texts: {}\".format(avg))\n","    print(\"20 most frequent words:\\n\")\n","    for i, f in ordered[:20]:\n","      print(\"{}\\t{}\".format(i, f))\n","    frqnt_words[label]=ordered\n","  return words_per_class, unique_words_per_class, documents_per_class, max_text_length, avg_words,frqnt_words\n","\n","words, unique_words, documents, max_text_length, avg_words, frequent_words = data_statistics(data, target_cols)\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"r_NMgiljmDxa","executionInfo":{"status":"ok","timestamp":1689570293037,"user_tz":240,"elapsed":3846,"user":{"displayName":"Omar Sharif","userId":"07937204899122819530"}},"outputId":"b14b8beb-1b26-42c7-9226-84bcd4cc8564"},"execution_count":64,"outputs":[{"output_type":"stream","name":"stdout","text":["873\n","to, 82.4838\n","the, 69.1179\n","and, 66.8240\n","my, 58.1863\n","for, 46.0499\n","me, 39.7738\n","in, 38.3213\n","it, 38.0853\n","have, 37.9868\n","of, 37.5625\n","on, 34.3533\n","im, 33.5412\n","is, 33.4947\n","this, 30.7390\n","they, 30.5789\n","that, 30.0432\n","but, 28.7563\n","get, 28.7147\n","with, 28.4071\n","so, 27.8540\n","suboxone, 27.1827\n","if, 26.1405\n","was, 26.0730\n","just, 24.9538\n","do, 24.8226\n","be, 24.1990\n","or, 24.1936\n","been, 23.5311\n","you, 23.4911\n","anyone, 22.5581\n","doctor, 21.6408\n","not, 21.6237\n","out, 21.2293\n","will, 21.0754\n","ive, 20.8922\n","any, 20.7107\n","script, 19.8391\n","day, 19.4472\n","up, 19.3387\n","them, 19.2031\n","bupe, 19.0797\n","what, 19.042